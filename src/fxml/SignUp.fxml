<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" fx:id="SignUpPanel" prefHeight="503.0" prefWidth="854.0" style="-fx-background-color: #fff;" stylesheets="@../styling/SignInStyling.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.SignUpController">
    <children>
        <AnchorPane prefHeight="503.0" prefWidth="382.0" style="-fx-background-color: #262721;" stylesheets="@../styling/SignInStyling.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <ImageView fitHeight="536.0" fitWidth="393.0" layoutX="-1.0" layoutY="-16.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../images/signUp.png" />
                    </image>
                </ImageView>
            </children>
        </AnchorPane>
        <Pane layoutX="474.0" layoutY="87.0" AnchorPane.bottomAnchor="86.0" AnchorPane.topAnchor="87.0">
            <children>
                <TextField fx:id="txtUserName" layoutX="7.0" prefHeight="34.0" prefWidth="275.0" promptText="user name" />
                <TextField fx:id="txtEmail" layoutX="8.0" layoutY="60.0" prefHeight="34.0" prefWidth="275.0" promptText="email" />
                <PasswordField fx:id="txtPassword" layoutX="8.0" layoutY="122.0" prefHeight="34.0" prefWidth="275.0" promptText="password" stylesheets="@../styling/SignInStyling.css" />
                <PasswordField fx:id="txtConfirmPassword" layoutX="8.0" layoutY="183.0" prefHeight="34.0" prefWidth="275.0" promptText="confirm password" stylesheets="@../styling/SignInStyling.css" />
                <Button fx:id="btnSignUp" layoutX="4.0" layoutY="270.0" mnemonicParsing="false" onMouseClicked="#signUp" prefHeight="60.0" prefWidth="275.0" stylesheets="@../styling/SignInStyling.css" text="Sign up" />
            <Label fx:id="lblUserErrors" alignment="CENTER" layoutX="6.0" layoutY="33.0" prefHeight="27.0" prefWidth="275.0" text="Label" textFill="WHITE">
               <font>
                  <Font name="Segoe UI" size="10.0" />
               </font>
            </Label>
            <Label fx:id="lblEmailErrors" alignment="CENTER" layoutX="7.0" layoutY="94.0" prefHeight="27.0" prefWidth="275.0" text="Label" textFill="WHITE">
               <font>
                  <Font name="Segoe UI" size="10.0" />
               </font>
            </Label>
            <Label fx:id="lblPasswordErrors" alignment="CENTER" layoutX="8.0" layoutY="156.0" prefHeight="27.0" prefWidth="275.0" text="Label" textFill="WHITE">
               <font>
                  <Font name="Segoe UI" size="10.0" />
               </font>
            </Label>
            <Label fx:id="lblConfirmPasswordErrors" alignment="CENTER" layoutX="11.0" layoutY="217.0" prefHeight="27.0" prefWidth="267.0" text="Label" textFill="WHITE">
               <font>
                  <Font name="Segoe UI" size="10.0" />
               </font>
            </Label>
            </children>
        </Pane>
      <FontAwesomeIcon fx:id="btnClose" glyphName="CLOSE" layoutX="819.0" layoutY="33.0" onMouseClicked="#closeWindow" size="30" />
      <FontAwesomeIcon fx:id="btnMin" glyphName="MINUS" layoutX="784.0" layoutY="33.0" onMouseClicked="#minWindow" size="27" />
      <FontAwesomeIcon glyphName="ANGLE_LEFT" layoutX="757.0" layoutY="36.0" onMouseClicked="#goBack" size="40" />
    </children>
</AnchorPane>
